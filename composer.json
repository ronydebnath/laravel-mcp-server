{
    "name": "ronydebnath/mcp-sdk",
    "description": "Model Context Protocol (MCP) implementation for Laravel",
    "keywords": [
        "laravel",
        "mcp",
        "model-context-protocol"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Laravel MCP Team",
            "email": "team@laravel.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0",
        "illuminate/support": "^10.0",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/http-client": "^6.0",
        "symfony/event-dispatcher": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.0",
        "vimeo/psalm": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Ronydebnath\\MCP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ronydebnath\\MCP\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ronydebnath\\MCP\\MCPServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
} 